      },
      {
        "pagePath": "pages/contacts/contacts",
        "text": "通讯录",
        "iconPath": "static/images/通讯录.png",
        "selectedIconPath": "static/images/通讯录-copy.png"
      },
      {
        "pagePath": "pages/discover/discover",
        "text": "发现",
        "iconPath": "static/images/发现.png",
        "selectedIconPath": "static/images/发现-copy.png"
      },
      {
        "pagePath": "pages/me/me",
        "text": "我",
        "iconPath": "static/images/我.png",
        "selectedIconPath": "static/images/我-copy.png"
      }
    ]
